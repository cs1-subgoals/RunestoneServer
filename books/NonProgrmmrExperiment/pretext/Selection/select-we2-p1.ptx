    <exercise xml:id="m-Select-WE2-P1-6">
        <statement>

            <p>Q6: What is the output of the following code?</p>
            <program language="java"><input>
if (false)
   System.out.println("it is true");
else
   System.out.println("it is false");
</input></program>

        </statement>
<choices>

            <choice>
                <statement>
                    <p>it is true</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice correct="yes">
                <statement>
                    <p>it is false</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>There is no output because a compiler error occurs</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>There is no output because an exception occurs</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>
</choices>

    </exercise>

