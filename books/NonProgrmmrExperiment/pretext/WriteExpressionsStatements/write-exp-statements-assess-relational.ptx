        <exercise>
            <statement>
                <p>Q1: z is odd 
                </p>
                <p>
                    <c>z % 2 </c><var/><c> 1</c>
                </p>
            </statement>
            <setup>
                <var>
                    <condition string="==$">
                        <feedback>
                            <p>Correct</p>
                        </feedback>
                    </condition>
                    <condition string=".*">
                        <feedback>
                            <p>Incorrect</p>
                        </feedback>
                    </condition>
                </var>
            </setup>
        </exercise>
