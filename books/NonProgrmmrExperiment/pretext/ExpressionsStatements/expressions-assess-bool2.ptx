    <exercise xml:id="assess-exp-bool2-1">
        <statement>

            <p>Q1: Given the following, what value is stored in variable <c>answer</c>?</p>
            <program language="java"><input>
int x = 5;
int y = 3;
int z = 1;
boolean answer;
answer = (x &lt; y) &amp;&amp; (z &lt; y);
        </input></program>
        </statement>
<choices>

            <choice>
                <statement>
                    <p>true</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice correct="yes">
                <statement>
                    <p>false</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>
</choices>

    </exercise>

