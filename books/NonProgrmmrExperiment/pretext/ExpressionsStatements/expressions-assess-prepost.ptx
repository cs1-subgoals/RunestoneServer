    <exercise xml:id="assess-exp-pp-4">
        <statement>

            <p>Q4: Given the following, what value is stored in variable delta?</p>
            <program language="java"><input>
int a = 0;
int b = 1;
int c = 2; 
int d = 3; 
int delta;
delta = d / --c - a % ++b;
        </input></program>
        </statement>
<choices>

            <choice correct="yes">
                <statement>
                    <p>3</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>2</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>1</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
