        <condition string="16">
            <feedback>
                <p>Correct</p>
            </feedback>
        </condition>
        <condition string='.*'>
            <feedback>
                <p>Incorrect</p>
            </feedback>
        </condition>
    </var>
</setup>
</exercise>
