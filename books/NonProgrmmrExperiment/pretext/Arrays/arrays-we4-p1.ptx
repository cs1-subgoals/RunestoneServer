            <exercise>
                <statement>
                    <p>Q37: Given the following code, what is the value in <c>sum</c> after execution? <var/>
                        
                    </p>
                    <program language="java"><input>
int [] alpha = {2, 4, 6, 8, 10, 12, 14};
int sum = 0;
for (int k = 0; k&lt;alpha.length; k+=2)
    sum = sum + alpha[k];
                        </input></program>
                </statement>
                <setup>
                    <var>
                        <condition string="32">
                            <feedback>
                                <p>Correct.</p>
                            </feedback>
                        </condition>
                        <condition string=".*">
                            <feedback><p>Incorrect.</p></feedback>
                        </condition>
                    </var>
                </setup>
            </exercise>
