    <exercise xml:id="m-Arrays-WE9-P1-49">
        <statement>

            <p>Q49: What does the following code do?</p>
            <program language="java"><input>
int target = /*  some value */ ;
int [] delta = {10, 20, 30, 40, 50, 60, 70, 80, 90};
boolean found = false;
int i, j;
for (i = 0; i &lt; delta.length &amp;&amp; !found; i++)
   if (delta[i] == target)
      found = true;
if (found) {
   for (j = i-1; j &lt; delta.length-1; j++)
      delta[j] = delta[j+1];
   delta[delta.length-1] = -999;
}
</input></program>

