    <exercise xml:id="m-Arrays-WE8-P1-45">
        <statement>

            <p>Q45: What are the contents of array alpha after this code has been executed?</p>
            <program language="java"><input>
int [] alpha = {10, 20, 30, 40, 50, 60, 70};
int start = alpha[alpha.length-1];
for (int i = 1; i &lt; alpha.length; i++)  {
   alpha[i] = alpha[i-1];
}
alpha[0] = start;
</input></program>

        </statement>
<choices>

            <choice>
                <statement>
                    <p>{20, 30, 40, 50, 60, 70, 10}</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>{10, 20, 30, 40, 50, 60, 70}</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>{10, 10, 20, 30, 40, 50, 60}</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice correct="yes">
                <statement>
                    <p>{70, 10, 10, 10, 10, 10, 10}</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>{10, 10, 10, 10, 10, 10, 10}</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>
</choices>

    </exercise>

