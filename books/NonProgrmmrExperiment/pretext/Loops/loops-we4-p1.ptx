    <exercise xml:id="m-Loops-WE4-P1-11">
        <statement>

            <p>Q11: What is the output of the following loop if the user input is: 10, 20, 30, 35, 40, -1</p>
            <program language="java"><input>
System.out.println("Enter a negative score to signal the end of the input.");
int total = 0, count = 0;
int score;
System.out.println("Score: ");
Scanner get = new Scanner(System.in);
score= get.nextInt();
while (score &gt;= 0) {
   count++;
   total += score;
   System.out.print("Score: ");
   score = get.nextInt();
}
System.out.println( (total * 1.0) / count);
</input></program>

        </statement>
<choices>

            <choice correct="yes">
                <statement>
                    <p>27.0</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>27</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>22.5</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>22</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>Compilation error</p>
                </statement>
