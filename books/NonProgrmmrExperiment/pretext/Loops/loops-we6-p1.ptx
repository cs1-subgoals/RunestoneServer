        <exercise xml:id="m-Loops-WE6-P1-15">
            <statement>

                <p>Q15: What is the output of the following loop?</p>
                <program language="java">
                    <input>
for (int m = 0; m &lt; 4; m++)
{
   for (int n = 5; n &gt;= 0; n--)
   {
      System.out.print("#");
   }
   System.out.println();
}
                    </input>
                </program>

            </statement>
            <choices>

                <choice>
                    <statement>
                        <p>5 lines of ###</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>3 lines of #####</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice correct="yes">
                    <statement>
                        <p>4 lines of ######</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>5 lines of ####</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>Compilation error</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>
            </choices>

        </exercise>

