        <exercise xml:id="m-Classes-WE2-P1-11">
            <statement>

                <p>Q11: Which of the following is NOT true about constructors?</p>

            </statement>
            <choices>

                <choice>
                    <statement>
                        <p>Constructors must be named the same name as the class</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>Default constructors have no parameters</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice correct="yes">
                    <statement>
                        <p>Classes can only have a single constructor</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>Constructors must be public</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>Constructors have no return type, not even void</p>
                    </statement>
                    <feedback>
                        <p/>
                    </feedback>
                </choice>
            </choices>

        </exercise>

        <exercise xml:id="m-Classes-WE2-P1-12">
