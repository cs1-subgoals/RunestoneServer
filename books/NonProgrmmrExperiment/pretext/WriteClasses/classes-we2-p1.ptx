                    <input>
public class Point {
   private int x;
   private int y;

   public Point (int one, int two) {/*LOGIC*/}
   public Point (int a, int b) {/*LOGIC*/}
}
                    </input>
                </program>

            </statement>
            <choices>

                <choice>
                    <statement>
                        <p>valid</p>
