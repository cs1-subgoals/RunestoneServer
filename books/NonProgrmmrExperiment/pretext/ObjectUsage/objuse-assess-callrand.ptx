    <exercise xml:id="assess-objuse-callrand-1">
        <statement>
            <p>Q1: Which of the following will correctly declare and instantiate a new variable to generate Random values?</p>
        </statement>
        <choices>

            <choice>
                <statement>
                    <p>Random rand = rand();</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>Random rand = rand(50);</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice correct="yes">
                <statement>
                    <p>Random rand = new Random();</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>Random rand = new Random(50);</p>
                </statement>
                <feedback>
                    <p/>
                </feedback>
            </choice>
        </choices>

    </exercise>

